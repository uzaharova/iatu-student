{
  "name": "Laravel Project",
  "image": "heroku/php",
  "scripts": {
    "postdeploy": "php artisan migrate --force"
  },
  "env": {
    "APP_ENV": "iatu-student",
    "DB_CONNECTION": "pgsql",
    "DB_DATABASE": "${DB_DATABASE}",
    "DB_HOST": "${DB_HOST}",
    "DB_PORT": "${DB_PORT}",
    "DB_PASSWORD": "${DB_PASSWORD}",
    "DB_USERNAME": "${DB_USERNAME}",
  }
}